#docker 
docker cp backup.sql mysql-container:/backup.sql
docker exec -i mysql-container sh -c 'exec mysql -u root -p"$MYSQL_ROOT_PASSWORD" your_database' < backup.sql

cat backup.sql | docker exec -i mysql-container sh -c 'mysql -u root -p"$MYSQL_ROOT_PASSWORD" your_database'

